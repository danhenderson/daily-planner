    "emotion": "^9.1.3",
    "emotion-theming": "^9.2.4",
    "minimal.css": "^1.0.1",
    "moment": "^2.22.2",
    "node-sass": "^4.9.0",
    "prop-types": "15.x",
    "react": "15.x || 16.x",
    "react-dom": "^16.4.1",
    "react-emotion": "^9.2.4",
    "react-redux": "^5.0.7",
    "react-swipe": "^5.1.1",
    "redux": "^4.0.0",
